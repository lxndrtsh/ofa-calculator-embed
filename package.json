{
  "name": "hpp-embed-starter",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "dev": "concurrently \"npm:dev:next\" \"npm:dev:loaders\"",
    "dev:next": "next dev",
    "dev:loaders": "npm-run-all --parallel dev:loader-impact dev:loader-community",
    "dev:loader-impact": "rollup -c packages/loader-impact/rollup.config.mjs -w",
    "dev:loader-community": "rollup -c packages/loader-community/rollup.config.mjs -w",
    "prebuild": "node -e \"const fs = require('fs'); const path = require('path'); const dir = path.join(process.cwd(), 'public', 'cdn'); if (!fs.existsSync(dir)) { fs.mkdirSync(dir, { recursive: true }); }\"",
    "build": "npm-run-all build:loaders build:next",
    "build:loaders": "npm-run-all --continue-on-error=false --sequential build:loader-impact build:loader-community",
    "build:loader-impact": "rollup -c packages/loader-impact/rollup.config.mjs",
    "build:loader-community": "rollup -c packages/loader-community/rollup.config.mjs",
    "build:next": "next build",
    "start": "next start",
    "lint": "eslint ."
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.947.0",
    "@hubspot/api-client": "^13.4.0",
    "lucide-react": "^0.552.0",
    "next": "14.2.5",
    "pdf-lib": "^1.17.1",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^20.12.8",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "concurrently": "^9.0.1",
    "eslint": "^9.8.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^4.21.0",
    "tslib": "^2.6.2",
    "typescript": "^5.4.5"
  }
}
